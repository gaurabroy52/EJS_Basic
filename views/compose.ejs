<%- include("partials/header");  -%>
<h1>Compose</h1>


<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="Text1">Title</label>
    <input name="postTitle"  id="Text1" class="form-control">
  </div>

  <div class="form-group">
    <label for="Text2">Post</label>
    <textarea name="postBody" rows="5" id="Text2" class="form-control"></textarea>
  </div>


  <button type="submit" name="composeButton" class="btn btn-success">Publish</button>
</form>

<%- include("partials/footer");  -%>
